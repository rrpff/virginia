// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Feed {
  id  String @id @default(uuid())
  url String
}

model FeedItem {
  id          String   @id @default(uuid())
  url         String
  title       String
  description String?
  image_url   String?
  timestamp   DateTime
}
